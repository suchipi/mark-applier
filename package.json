{
  "name": "mark-applier",
  "version": "0.1.0",
  "description": "Markdown-to-Website Generator, GitHub README style",
  "type": "module",
  "main": "dist/index.js",
  "types": "src/index.ts",
  "bin": "dist/cli.js",
  "scripts": {
    "build": "rm -rf dist && tsc && chmod +x dist/cli.js",
    "test": "npm run compile-tests && concurrently 'npm run host-tests' 'npm run open-test-results'",
    "compile-tests": "tests/generate-all.js",
    "host-tests": "http-server -p 8080 tests/output",
    "open-test-results": "open-cli http://localhost:8080"
  },
  "keywords": [
    "website",
    "markdown",
    "generator",
    "static",
    "site"
  ],
  "author": "Lily Skye <me@suchipi.com>",
  "license": "MIT",
  "dependencies": {
    "@suchipi/macaroni": "^0.2.0",
    "@wooorm/starry-night": "^2.0.0",
    "clefairy": "^0.4.0",
    "github-markdown-css": "^5.2.0",
    "gray-matter": "^4.0.3",
    "hast": "^1.0.0",
    "hast-util-to-string": "^2.0.0",
    "kleur": "^4.1.5",
    "rehype-stringify": "^9.0.3",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.2"
  },
  "devDependencies": {
    "@types/node": "^18.16.2",
    "concurrently": "^8.0.1",
    "glomp": "^3.1.1",
    "http-server": "^14.1.1",
    "open-cli": "^7.2.0",
    "path-less-traveled": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
